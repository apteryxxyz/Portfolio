        "react-icons": "^4.3.1",
        "react-is": "^17.0.2",
        "styled-components": "^5.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "eslint": "^8.9.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/apteryxxyz/apteryx.xyz.git"
    }
}